<view class="aidedtools"><view class="type"><view data-event-opts="{{[['tap',[['typeChoose',[0]]]]]}}" class="{{['typeName',(type==0)?'typeChoose':'']}}" bindtap="__e">伤残赔偿<view hidden="{{!(type==0)}}" class="typeLine"></view></view><view data-event-opts="{{[['tap',[['typeChoose',[1]]]]]}}" class="{{['typeName',(type==1)?'typeChoose':'']}}" bindtap="__e">死亡赔偿<view hidden="{{!(type==1)}}" class="typeLine"></view></view></view><view style="height:80rpx;"></view><view class="content"><view class="basicInfo"><block wx:for="{{basicInfo}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{item.isShow==2||item.isShow==type}}"><view class="inputBox"><text class="title">{{item.name}}</text><block wx:if="{{item.isRequired}}"><text class="required">*</text></block><block wx:if="{{item.type=='input'}}"><view class="input"><input type="number" placeholder="{{item.placeholder}}" data-event-opts="{{[['input',[['__set_model',['$0','value','$event',[]],[[['basicInfo','',index]]]]]]]}}" value="{{item.value}}" bindinput="__e"/><text class="unit">{{item.unit}}</text></view></block><block wx:if="{{item.type=='select'}}"><view class="input select"><picker value="{{item.value}}" range="{{item.list}}" data-event-opts="{{[['change',[['selectChange',['$event']]]],['tap',[['selectClick',[index]]]]]}}" bindchange="__e" bindtap="__e"><text>{{item.list[item.value]}}</text></picker><image class="type_more" src="../../static/tool_4/common/icon_more.png" mode></image></view></block><block wx:if="{{item.type=='radio'}}"><view class="input radio"><block wx:for="{{item.list}}" wx:for-item="item_" wx:for-index="index_" wx:key="index_"><view data-event-opts="{{[['tap',[['link',[index,index_]]]]]}}" class="radioBox" bindtap="__e"><block wx:if="{{item.value!=index_}}"><image src="../../static/tool_4/common/icon_selsect.png" mode></image></block><block wx:else><image src="../../static/tool_4/common/icon_select_T.png" mode></image></block><text>{{item_}}</text></view></block></view></block><view class="splitLine"></view></view></block></block></view><view class="custodian"><view class="isNeedBox"><text class="isNeedText">是否有需扶养人</text><view data-event-opts="{{[['tap',[['need',[0]]]]]}}" class="radioBox" bindtap="__e"><block wx:if="{{isNeed!=0}}"><image src="../../static/tool_4/common/icon_selsect.png" mode></image></block><block wx:else><image src="../../static/tool_4/common/icon_select_T.png" mode></image></block><text>是</text></view><view data-event-opts="{{[['tap',[['need',[1]]]]]}}" class="radioBox" bindtap="__e"><block wx:if="{{isNeed!=1}}"><image src="../../static/tool_4/common/icon_selsect.png" mode></image></block><block wx:else><image src="../../static/tool_4/common/icon_select_T.png" mode></image></block><text>否</text></view></view><block wx:for="{{custodianInfo}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="custodianBox"><image class="custodianDel" src="../../static/tool_4/common/icon_delete.png" mode data-event-opts="{{[['tap',[['delCustodian',[index]]]]]}}" bindtap="__e"></image><block wx:for="{{item}}" wx:for-item="item_" wx:for-index="index_" wx:key="index_"><view class="custodianInputBox"><text class="title">{{item_.name}}</text><block wx:if="{{item_.type=='input'}}"><view class="input"><input type="number" placeholder="{{item_.placeholder}}" data-event-opts="{{[['input',[['__set_model',['$0','value','$event',[]],[[['custodianInfo','',index],['','',index_]]]]]]]}}" value="{{item_.value}}" bindinput="__e"/><text class="unit">{{item_.unit}}</text></view></block><block wx:if="{{item_.type=='radio'}}"><view class="input radio"><block wx:for="{{item_.list}}" wx:for-item="item_r" wx:for-index="index_r" wx:key="index_r"><view data-event-opts="{{[['tap',[['work',[index,index_,index_r]]]]]}}" class="radioBox" bindtap="__e"><block wx:if="{{item_.value!=index_r}}"><image src="../../static/tool_4/common/icon_selsect.png" mode></image></block><block wx:else><image src="../../static/tool_4/common/icon_select_T.png" mode></image></block><text>{{item_r}}</text></view></block></view></block><view class="splitLine"></view></view></block></view></block><block wx:if="{{isNeed==0}}"><view data-event-opts="{{[['tap',[['addCustodian',['$event']]]]]}}" class="custodianAdd" bindtap="__e"><image src="../../static/tool_4/common/icon_add.png" mode></image><text>添加扶养人</text></view></block></view><view data-event-opts="{{[['tap',[['goCalc',['$event']]]]]}}" class="calc" bindtap="__e">去计算</view><view class="result"><view class="title"><image src="../../static/tool_4/common/icon_calc.png" mode></image><text>计算结果</text></view><block wx:for="{{result}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{item.type==type||item.type==2}}"><view class="fee"><text>{{item.name}}</text><view class="amount"><text class="unit">{{item.value+" 元"}}</text></view><view class="amountLine"></view></view></block></block><block wx:for="{{besupport}}" wx:for-item="item" wx:for-index="index_besupport" wx:key="index_besupport"><block wx:if="{{besupport.length}}"><view class="fee"><text>{{"第"+(index_besupport+1)+"位被抚养人"}}</text><view class="amount"><text class="unit">{{item.fee+"元"}}</text></view><view class="amountLine"></view></view></block></block></view></view></view>